{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Producer": {
    "BootstrapServers": "localhost:9092",
    "ClientId": "WalletApi",
    "Acks": "All",
    "MessageTimeoutMs": 300000,
    "BatchNumMessages": 10000,
    "LingerMs": 5,
    "CompressionType": "Gzip"
  },
  "SchemaRegistry": {
    "URL": "http://localhost:8081"
  },
  "Redis": {
    "Host": "localhost:6379"
  }
}